{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "resultCode": {
      "type": "integer"
    },
    "status": {
      "type": "object",
      "properties": {
        "statusCode": {
          "type": "integer"
        },
        "statusDesc": {
          "type": "string"
        }
      },
      "required": [
        "statusCode",
        "statusDesc"
      ]
    },
    "response": {
      "type": "object",
      "properties": {
        "statusProcess": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "packageOfServices": {
          "type": "string"
        },
        "packageOfServicesDescription": {
          "type": "string"
        }
      },
      "required": [
        "statusProcess",
        "id",
        "packageOfServices",
        "packageOfServicesDescription"
      ]
    },
    "accountInfo": {
      "type": "object",
      "properties": {
        "accountNumber": {
          "type": "string"
        },
        "corrAccountNumber": {
          "type": "string"
        },
        "bic": {
          "type": "string"
        }
      },
      "required": [
        "accountNumber",
        "corrAccountNumber",
        "bic"
      ]
    }
  },
  "required": [
    "resultCode",
    "status",
    "response",
    "accountInfo"
  ]
}